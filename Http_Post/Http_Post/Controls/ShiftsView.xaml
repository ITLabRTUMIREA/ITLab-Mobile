<?xml version="1.0" encoding="UTF-8"?>
<ViewCell xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Http_Post.Controls.ShiftsView">
    <StackLayout x:Name="stackLayout"
                 Style="{DynamicResource themeStack}">
        <Label Text="{Binding BeginTime, StringFormat=' {0:dd MMMM, yyyy. HH:mm}'}"
               Style="{DynamicResource themeLabel}"/>
        <Label Text="{Binding EndTime, StringFormat=' {0:dd MMMM, yyyy. HH:mm}'}"
               Style="{DynamicResource themeLabel}"
               Margin="0,0,0,20"/>
    </StackLayout>
</ViewCell>