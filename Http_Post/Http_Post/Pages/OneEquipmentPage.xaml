<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Http_Post.Pages.OneEquipmentPage">
    <ContentPage.Content>
        <ScrollView>
            <StackLayout Spacing="5"
                         Margin="20,20,0,20">
                <Label x:Name="lblTypeTitle"/>
                <Label x:Name="lblType"
                   Margin="0,0,0,20"/>
                <Label x:Name="lblNumberTitle"/>
                <Label x:Name="lblNumber"
                   Margin="0,0,0,20"/>
                <Label x:Name="lblDescriptionTitle"/>
                <Label x:Name="lblDescription"
                   Margin="0,0,0,20"/>
                <Label x:Name="lblOwnerTitle"/>
                <Label x:Name="lblOwner"
                   Margin="0,0,0,20"/>
            </StackLayout>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>